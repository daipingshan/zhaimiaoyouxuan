/* pages/orderdetails/orderdetails.wxss */

.cord-list {
    display: block;
    width: 100%;
    padding: 30rpx 28rpx 10rpx;
    box-sizing: border-box;
    margin-bottom: 12rpx;
    background: #fff;
}

.odr-item {
    display: block;
    width: 100%;
    padding: 190rpx 12rpx 0;
    box-sizing: border-box;
    background: #f7f7fa;
}

.item-title {
    color: #808080;
}

.item-desc {
    font-weight: 400;
    margin-top: 10rpx;
    color: #262626;
}

.money-con-item{
	text-align: center;
}

.money-con-title {
    color: #808080;
	display: block;
	margin-bottom:10rpx;
}

.money-con-price {
    font-weight: bold;
    margin-top: 10rpx;
    color: #262626;
	font-size: 26rpx;
}

.statis-con {
    display: block;
    width: 100%;
    height: 168rpx;
    position: fixed;
    background: #fff;
    box-sizing: border-box;
    left: 0;
    z-index: 9999;
}

.statistics {
    padding: 15rpx 30rpx 15rpx 46rpx;
    box-sizing: border-box;
    width: 100%;
    background: #fff;
}

.data-bind {
    height: 44rpx;
    position: relative;
}

.data-fl {
    float: left;
}

.data-fl .jt-up, .data-fl text {
    float: left;
}

.data-fl .time {
    font-size: 24rpx;
    color: #010101;
    float: left;
}

.data-fl .line {
    margin: 0 20rpx;
}

.jt-up {
    width: 30rpx;
    height: 30rpx;
    margin-left: 10rpx;
    float: left;
}

.search-img-r {
    float: right;
    width: 44rpx;
    height: 44rpx;
}

.search-img-r image {
    width: 100%;
    height: 100%;
}

.take-in-pay {
    overflow: hidden;
    padding-top: 10rpx;
}

.take-in {
    width: 33.33%;
    display: block;
    text-align: center;
    float: left;
}

take-pay-r {
    float: right;
}

.take-in-tit {
    display: block;
    font-size: 24rpx;
    color: #141414;
    margin-bottom: 10rpx;
}

.take-in-mn {
    font-size: 30rpx;
    color: #141414;
}

.take-pay-r text {
    color: #010101;
}

.search-panel {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 60rpx;
    left: 0;
    background: rgba(0, 0, 0, 0.3);
    z-index: 100;
}

.search-nav {
    display: block;
    width: 100%;
    background: #fff;
    padding: 35rpx 30rpx 20rpx;
    box-sizing: border-box;
}

.nav-cont {
    position: relative;
    overflow: hidden;
    display: flex;
    justify-content: space-around;
}

.nav-cont view {
    display: block;
    height: 44rpx;
    line-height: 44rpx;
    width: 150rpx;
    text-align: center;
    float: left;
    border: 2rpx solid #f6012d;
    font-size: 24rpx;
    color: #f00;
    border-radius: 24rpx;
    margin-left: 15rpx;
    margin-bottom: 48rpx;
}

.nav-cont .whole {
    width: 102rpx;
    margin-left: 0;
}

.nav-cont.all.state .all, .nav-cont.wait_settle.state .wait_settle,
.nav-cont.settle.state .settle, .nav-cont.fail.state .fail,
.nav-cont.all.sorce .all, .nav-cont.self.sorce .self, .nav-cont.son.sorce .son,
.nav-cont.group_leader.sorce .group_leader, .nav-cont.partner.sorce .partner {
    background: #f00;
    color: #fff;
}

.nav-cont text {
    display: block;
    height: 48rpx;
    line-height: 48rpx;
    font-size: 24rpx;
    color: #141414;
    position: absolute;
    left: 0;
    top: 0;
}

.sure-btn {
    display: block;
    width: 300rpx;
    height: 62rpx;
    text-align: center;
    line-height: 62rpx;
    background: #f00;
    color: #fff;
    font-size: 24rpx;
    border-radius: 30rpx;
    margin: auto;
}

.data-picker {
    overflow: hidden;
    float: left;
}

.data-picker picker {
    font-size: 24rpx;
    color: #010101;
    float: left;
}

.odr-con {
    padding: 20rpx 20rpx 15rpx 20rpx;
    background: #fff;
    width: 100%;
    display: block;
    box-sizing: border-box;
    margin-bottom: 15rpx;
}
.odr-img {
    width: 168rpx;
    height: 168rpx;
    float: left;
    margin-right: 20rpx;
}

.pro-title {
    font-size: 26rpx;
    color: #444;
    display: block;
    line-height: 30rpx;
    font-weight: bold;
}

.money-con {
    padding-bottom: 25rpx;
    overflow: hidden;
    font-size: 24rpx;
    margin-top: 20rpx;
    display: flex;
    justify-content: space-around;
}

.font-blod {
    font-weight: bold;
    font-size: 30rpx;
}

.stats-con {
    overflow: hidden;
    margin-bottom: 25rpx;
}

.left-btn {
    width: 40%;
    float: left;
    text-align: center;
    font-size: 24rpx;
}

.yfk {
    color: #00a6b5;
}

.zgtg {
    color: #ff8400;
}

.odr-detail {
    display: flex;
    justify-content: space-around;
    text-align: center;
    border-bottom: 1px solid #eee;
    padding-bottom: 20rpx;
    margin-bottom: 10rpx;
    font-size: 25rpx;
    width: 100%;
}

.order_type {
    margin-left: 10rpx;
    color: #8c8c8c;
}

.order-detail-item {
    width: 33%;
}

.gray {
    color: #9a9a9a;
}

.blue {
    color: #3bb5bb;
}

.brown {
    color: #a02e2e;
}

.odr-time {
    overflow: hidden;
    font-size: 18rpx;
    color: #8c8c8c;
}

.creation-time {
    float: left;
    margin-left: 15rpx;
}

.end-time {
    float: right;
    margin-right: 15rpx;
}

.yjs {
    color: #f00;
}

.fc {
    color: #aa3c00;
}

.empty {
    font-size: 24rpx;
    display: block;
    padding: 20rpx 0;
    color: #ccc;
    text-align: center;
}
