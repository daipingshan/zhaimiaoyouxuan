/* pages/jdItem/item.wxss */

@font-face {
  font-family: 'LCDMono';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: bold;
  font-style: normal;
}

.time-font {
  font-family: LCDMono;
}

.no-begin {
  width: 100%;
  position: relative;
  background: #1a0026;
}

.no-begin .img image {
  width: 750rpx;
  height: 1070rpx;
}

.no-begin .time-down {
  position: absolute;
  top: 850rpx;
  left: 0;
  width: 100%;
  height: 100rpx;
  line-height: 80rpx;
  text-align: center;
  color: #ffe878;
  font-size: 70rpx;
}

.no-begin .time-down text {
  font-size: 24rpx;
  margin-left: 8rpx;
  margin-right: 22rpx;
}

.no-begin .wei-xin {
  position: absolute;
  top: 950rpx;
  left: 0;
  width: 100%;
  line-height: 50rpx;
  font-size: 24rpx;
  color: #ffdcf8;
  text-align: center;
}

.no-begin .copy-weixin {
  position: absolute;
  top: 1000rpx;
  left: 0;
  width: 100%;
  line-height: 50rpx;
  font-size: 24rpx;
  color: #ffdcf8;
  text-align: center;
}

.no-begin .copy-weixin .copy-weixin-btn {
  display: inline-block;
  width: 100rpx;
  height: 40rpx;
  line-height: 40rpx;
  border-radius: 20rpx;
  border: 1px solid #ffe878;
  color: #ffe878;
}

.jd-item {
  width: 100%;
  font-size: 24rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  background: #520066;
}

.top-img {
  width: 100%;
  display: block;
  position: relative;
}

.top-img .gui-ze {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: 380rpx;
  right: 10rpx;
  z-index: 10;
}

.top-img .free-item {
  width: 300rpx;
  height: 100rpx;
  position: absolute;
  top: 380rpx;
  left: 225rpx;
  z-index: 10;
}

.top-img .free-ze {
  width: 300rpx;
  height: 100rpx;
  position: absolute;
  top: 380rpx;
  left:225rpx;
  z-index: 10;
}

.top-img image {
  width: 750rpx;
  height: 670rpx;
}

.item-tab {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
}

.item-tab .item-cate {
  width: 50%;
  text-align: center;
  margin: auto;
  color: #fff;
  font-size: 30rpx;
  font-weight: bold;
  position: relative;
}

.item-tab.free .free::after, .item-tab.new .new::after {
  display: block;
  content: "";
  width: 50%;
  height: 5px;
  background: #fff;
  position: absolute;
  left: 25%;
  bottom: -20rpx;
}

.item-list {
  width: 92%;
  background: #fff;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  min-height: 500rpx;
  border-radius: 15rpx;
  margin-bottom: 50rpx;
}

.item-list .after-info{
  width: 100%;
  height: 100%;
  margin: auto;
  line-height: 50rpx;
  padding: 20rpx;
  font-size: 24rpx;
}

.goods-list {
  width: 200%;
}

.goods-item {
  height: 200rpx;
  flex-wrap: nowrap;
  margin-bottom: 10rpx;
  box-shadow: 3px 3px 10px #ccc;
  background-color: #fff;
  width: 100%;
}

.goods-img {
  float: left;
  width: 200rpx;
}

.goods-img image {
  margin: 10rpx;
  height: 180rpx;
  width: 180rpx;
}

.goods-detail {
  float: right;
  padding: 10rpx 20rpx;
  width: 400rpx;
}

.goods-name {
  font-size: 24rpx;
  height: 80rpx;
  display: block;
  line-height: 40rpx;
  overflow: hidden;
}

.goods-name .topCate {
  color: #fff;
  background: #bc0031;
  font-size: 22rpx;
  line-height: 35rpx;
  width: 55rpx;
  display: inline-block;
  text-align: center;
  border-radius: 40rpx;
  float: left;
  margin-top: 2rpx;
  margin-right: 10rpx;
}

.goods-coupon {
  width: 100%;
  height: 40rpx;
  margin: 20rps;
  position: relative;
}

.goods-coupon image {
  height: 40rpx;
  width: 130rpx;
  right: 0px;
  z-index: -1;
}

.goods-coupon text {
  z-index: 10;
  position: absolute;
  left: 50rpx;
  color: #bc0031;
  text-align: left;
  top: 0;
  font-size: 24rpx;
  line-height: 40rpx;
}

.price-num {
  position: relative;
  height: 50rpx;
  font-size: 20rpx;
  line-height: 50rpx;
  color: #808080;
}

.now-price {
  color: red;
}

.subsidy_money {
  float: right;
  color: #fff;
  background: #bc0031;
  padding: 0 20rpx;
  border-radius: 30rpx 30rpx 30rpx 0rpx;
}

.share, .back-home {
  width: 100rpx;
  height: 100rpx;
  position: fixed;
  z-index: 11;
  right: 2%;
  text-align: center;
}

.share {
  top: 700rpx;
}

.back-home {
  top: 820rpx;
}

.share image {
  width: 100rpx;
  height: 100rpx;
}

.back-home image {
  width: 80rpx;
  height: 80rpx;
}

.modal-box {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0px;
  background: rgba(0, 0, 0, 0.8);
  overflow: hidden;
  z-index: 99;
}

.modal-body {
  position: relative;
}

.modal-content {
  margin-left: auto;
  margin-right: auto;
  width: 86%;
  overflow: hidden;
  background: #fff;
  padding: 0;
}

.modal-content canvas {
  width: 100%;
  height: 100%;
  z-index: 0;
}

.close {
  position: fixed;
  top: 33px;
  right: 7%;
  z-index: 999;
  width: 40rpx;
  height: 40rpx;
}

.footer {
  height: 7%;
  margin-right: 7%;
  margin-left: 7%;
  width: 86%;
  background: red;
  bottom: 0;
  position: fixed;
  display: flex;
  justify-content: space-around;
  z-index: 99;
}

.footer .footer-btn {
  width: 50%;
  text-align: center;
  font-size: 30rpx;
  font-family: '平方特细';
  color: #fff;
}

.footer-close {
  background: #808080;
}

.footer-copy {
  background: #e6de3d;
}

.footer-save {
  background: #febd99;
}

.footer-show {
  background: #fe5d39;
}
